{
  "name": "@aizen/shared",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "description": "Aizen vNE Shared Types and Utilities",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./utils/*": {
      "types": "./dist/utils/*.d.ts",
      "default": "./dist/utils/*.js"
    },
    "./lib/*": {
      "types": "./dist/lib/*.d.ts",
      "default": "./dist/lib/*.js"
    },
    "./types/*": {
      "types": "./dist/types/*.d.ts",
      "default": "./dist/types/*.js"
    },
    "./test/*": {
      "types": "./dist/test/*.d.ts",
      "default": "./dist/test/*.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .ts,.tsx --ignore-pattern '*.test.ts' --ignore-pattern 'test-*.ts'",
    "lint:fix": "eslint src --ext .ts,.tsx --fix --ignore-pattern '*.test.ts' --ignore-pattern 'test-*.ts'",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.0.0",
    "@types/express": "^5.0.3",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.56.0",
    "redis": "^5.8.2"
  }
}
