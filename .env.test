# Test Environment Variables
NODE_ENV=test

# Test Supabase Configuration (use same as dev for now, can be separate test project)
TEST_SUPABASE_URL=${SUPABASE_URL}
TEST_SUPABASE_ANON_KEY=${SUPABASE_ANON_KEY}
TEST_SUPABASE_SERVICE_KEY=${SUPABASE_SERVICE_KEY}
TEST_DATABASE_URL=${DATABASE_URL}

# Test Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6380

# Test API Configuration
API_GATEWAY_URL=http://localhost:3100
NEXT_PUBLIC_API_URL=http://localhost:3100

# Test Device Configuration
DEVICE_ID=TEST-DEVICE-001
DEVICE_SECRET=test-device-secret

# Test AI Configuration
ENABLE_AI_MOCK_MODE=true
MOCK_RESPONSE_DELAY=100

# Disable external services in tests
DISABLE_RATE_LIMITING=true
NEXT_PUBLIC_DISABLE_ANALYTICS=true

# Test timeouts
TEST_TIMEOUT=30000
TEST_RETRY_COUNT=2